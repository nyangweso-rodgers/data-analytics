   ],
   "source": [
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(projection='3d')\n",
    "\n",
    "ax.scatter(1, 1, 1)\n",
    "ax.scatter(2, 2, 1)\n",
    "ax.scatter(2, 3, 0)\n",
    "\n",
    "plt.show()"
   ]
  }
